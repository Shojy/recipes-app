<!DOCTYPE html>
<html>
<head>
  <title>Recipes</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <nav class="navbar navbar-default">
    <div class="container">
      <div class="navbar-brand"><%= link_to "Recipes", root_path %></div>
    </div>
  </nav>

<% if notice %>
<p><%= notice %></p>
<% end %>
<%= yield %>

</body>
</html>
